{
    "name": "konceiver/passphrase",
    "description": "Convert bytes to a human readable strings",
    "keywords": [
        "konceiver",
        "passphrase"
    ],
    "homepage": "https://github.com/konceiver/passphrase",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@konceiver.dev",
            "homepage": "https://konceiver.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "konceiver/laravel-stan": "^1.1",
        "konceiver/stan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Konceiver\\Passphrase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konceiver\\Passphrase\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --coverage --min=100"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
